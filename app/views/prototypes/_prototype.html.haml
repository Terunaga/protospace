.col-sm-4.col-md-3.proto-content
  .thumbnail
    = link_to image_tag("sample.png"), prototype_path(prototype)
    .caption
      %h3= prototype.title
      .proto-meta
        .proto-user
          = link_to "by #{prototype.user.username}", user_path(prototype.user_id)
        .proto-posted
          = prototype.created_at
      %ul.proto-tag-list.list-inline
        %li 
          = link_to "Ipad", "#", class: "btn btn-default"
        %li 
          = link_to "wireframe", "#", class: "btn btn-default"

